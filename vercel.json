{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "develop": true,
      "qa": true,
      "uat": true,
      "prod": true
    }
  },
  "github": {
    "enabled": true,
    "silent": false
  }
}
